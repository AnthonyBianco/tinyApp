  <nav class="navbar navbar-expand-md navbar-dark bg-success">
    <a class="navbar-brand" href="/urls">TinyApp</a>
    <button class="navbar-toggler" type="button" data-toggle="collapse" data-target="#navbarNavAltMarkup" aria-controls="navbarNavAltMarkup" aria-expanded="false" aria-label="Toggle navigation">
      <span class="navbar-toggler-icon"></span>
    </button>
    <div class="collapse navbar-collapse" id="navbarNavAltMarkup">
      <div class="navbar-nav">
        <a class="nav-item nav-link" href="/urls">My URLs</a>
        <a class="nav-item nav-link" href="/urls/new">Create New URL</a>
        
        <!-- THIS IS MY LOGIN LINK-->
        <a class="nav-item nav-link" href="/loginFile">LOGIN</a>
         <!-- THIS IS MY LOGIN LINK-->
          
         <!-- THIS IS MY REGISTER LINK-->
        <a class="nav-item nav-link" href="/register">REGISTER</a>
        <!-- THIS IS MY REGISTER LINK-->

      </div>
    </div>
    <!-- IF USER NOT LOGGED IN DISPLAY -->
    <% if(user_id === undefined){ %>
      <form class="form-inline" action="/loginFile" method="POST">
        <div class="form-group mb-2">
          <label for="Username"><%= user_id %></label>
        </div>
      </form>
      <!--  ELSE USER LOGGED IN DISPLAY -->
      <% } else { %>  
      <h6>Hello, <label> </label> </h6>
      <form class="form-inline" action="/logout" method="POST">
        <div class="form-group mb-2">
          <label style="margin-right: 1rem" for="Username"><%= user_id.email %></label>
          <button type="submit" class="btn btn-primary">Logout</button>
        </div>
      </form>
   <% } %>
    
    
  </nav>
</header>

